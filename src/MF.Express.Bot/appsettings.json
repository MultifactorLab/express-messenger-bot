{
  "ExpressBot": {
    "BotId": "YOUR_BOT_ID_UUID_HERE",
    "BotSecretKey": "YOUR_BOT_SECRET_KEY_HERE",
    "BotXApiBaseUrl": "https://your-express-server.com",
    "ExpectedIssuer": "your-express-server.com",
    "RequestTimeoutSeconds": 30,
    "ProtoVersion": 4,
    "EnableDetailedLogging": false
  },
  "MultifactorApi": {
    "BaseUrl": "https://your-multifactor-api.com",
    "ApiKey": "YOUR_MULTIFACTOR_API_KEY",
    "TimeoutSeconds": 30,
    "RetryCount": 3,
    "UserChatInfoEndpoint": "/api/bot/user-chat-info",
    "AuthorizationResultEndpoint": "/api/bot/authorization-result"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MF.Express.Bot": "Information",
      "MF.Express.Bot.Api.Middleware.BotXJwtValidationMiddleware": "Debug"
    }
  },
  "AllowedHosts": "*"
}
