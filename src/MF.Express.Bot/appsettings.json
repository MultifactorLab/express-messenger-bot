{
  "ExpressBot": {
    "BotToken": "YOUR_BOT_TOKEN_HERE",
    "ApiBaseUrl": "https://your-express-server.com/api/v1/",
    "WebhookUrl": "https://your-bot-domain.com/webhook",
    "WebhookSecret": "your_webhook_secret",
    "RequestTimeoutSeconds": 30
